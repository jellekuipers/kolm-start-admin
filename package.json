{
  "name": "kolm-start-admin",
  "description": "A TanStack Start + better-auth admin starter with Prisma ORM, React Aria, Tailwind, i18next.",
  "author": "Jelle Kuipers <info@jellekuipers.com> (https://jellekuipers.com)",
  "version": "1.3.3",
  "type": "module",
  "keywords": [
    "better-auth",
    "biomejs",
    "prisma",
    "react-aria",
    "i18next",
    "tailwindcss",
    "tanstack-start",
    "typescript"
  ],
  "scripts": {
    "build": "vite build && tsc --noEmit",
    "check": "npx @biomejs/biome check --write .",
    "db:dev": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:reset": "prisma migrate reset --skip-seed --force && npx prisma db push && npx prisma db seed",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "dev": "vite dev",
    "format": "npx @biomejs/biome format --write .",
    "lint": "npx @biomejs/biome lint --write .",
    "postinstall": "prisma generate",
    "start": "vite start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@phosphor-icons/react": "^2.1.10",
    "@prisma/client": "^6.16.3",
    "@t3-oss/env-core": "^0.13.8",
    "@tanstack/match-sorter-utils": "^8.19.4",
    "@tanstack/react-devtools": "^0.7.2",
    "@tanstack/react-form": "^1.23.5",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-router": "^1.132.33",
    "@tanstack/react-router-devtools": "^1.132.33",
    "@tanstack/react-router-ssr-query": "^1.132.33",
    "@tanstack/react-start": "^1.132.34",
    "@tanstack/react-table": "^8.21.3",
    "better-auth": "^1.3.26",
    "i18next": "^25.5.3",
    "react": "^19.2.0",
    "react-aria": "^3.44.0",
    "react-aria-components": "^1.13.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.0.0",
    "slugify": "^1.6.6",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.5",
    "@tailwindcss/vite": "^4.1.14",
    "@types/node": "^24.6.2",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@vitejs/plugin-react": "^5.0.4",
    "dotenv": "^17.2.3",
    "prisma": "^6.16.3",
    "tailwindcss": "^4.1.14",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-react-aria-components": "^2.0.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "^7.1.9",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
