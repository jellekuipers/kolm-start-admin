{
  "name": "kolm-start-admin",
  "description": "A TanStack Start + better-auth admin starter with Prisma ORM, React Aria, Tailwind, i18next.",
  "author": "Jelle Kuipers <info@jellekuipers.com> (https://jellekuipers.com)",
  "version": "2.0.1",
  "type": "module",
  "keywords": [
    "better-auth",
    "biomejs",
    "prisma",
    "react-aria",
    "i18next",
    "tailwindcss",
    "tanstack-start",
    "typescript"
  ],
  "scripts": {
    "build": "vite build",
    "check": "npx @biomejs/biome check --write .",
    "db:dev": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:reset": "prisma migrate reset && prisma db push && prisma db seed",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "dev": "vite dev",
    "format": "npx @biomejs/biome format --write .",
    "lint": "npx @biomejs/biome lint --write .",
    "postinstall": "prisma generate",
    "start": "vite start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@phosphor-icons/react": "^2.1.10",
    "@prisma/adapter-better-sqlite3": "^7.2.0",
