    "@tanstack/react-table": "^8.21.3",
    "better-auth": "^1.4.9",
    "i18next": "^25.7.3",
    "react": "^19.2.3",
    "react-aria": "^3.45.0",
    "react-aria-components": "^1.14.0",
    "react-dom": "^19.2.3",
    "react-i18next": "^16.5.0",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "zod": "^4.2.1"
