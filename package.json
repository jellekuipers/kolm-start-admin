{
  "name": "kolm-start-admin",
  "description": "A TanStack Start + better-auth admin starter, with Drizzle ORM, tRPC, Radix Themes.",
  "author": "Jelle Kuipers <info@jellekuipers.com> (https://jellekuipers.com)",
  "version": "0.9.1",
  "type": "module",
  "keywords": [
    "better-auth",
    "drizzle-orm",
    "tanstack-start",
    "tRPC",
    "typescript"
  ],
  "scripts": {
    "build": "vinxi build",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:reset": "npx tsx app/db/reset.ts ",
    "db:seed": "npx tsx app/db/seed.ts ",
    "db:seed:dev": "npx tsx app/db/seed.dev.ts ",
    "db:studio": "drizzle-kit studio",
    "dev": "vinxi dev",
    "format": "prettier --write .",
    "lint": "eslint app",
    "postinstall": "drizzle-kit push",
    "start": "node --env-file=.env ./node_modules/.bin/vinxi start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@radix-ui/react-icons": "1.3.2",
    "@radix-ui/themes": "3.2.1",
    "@tailwindcss/postcss": "^4.0.13",
    "@tanstack/match-sorter-utils": "8.19.4",
    "@tanstack/react-form": "1.0.5",
    "@tanstack/react-query": "5.67.3",
    "@tanstack/react-query-devtools": "5.67.3",
    "@tanstack/react-router": "1.114.17",
    "@tanstack/react-router-with-query": "1.114.17",
    "@tanstack/react-start": "1.114.17",
    "@tanstack/react-table": "8.21.2",
    "@tanstack/router-devtools": "1.114.18",
    "@trpc/client": "11.0.0-rc.828",
    "@trpc/server": "11.0.0-rc.828",
    "@trpc/tanstack-react-query": "11.0.0-rc.828",
    "better-auth": "1.2.3",
    "dotenv": "16.4.7",
    "drizzle-orm": "0.40.0",
    "drizzle-seed": "0.3.1",
    "pg": "8.14.0",
    "postcss": "^8.5.3",
    "react": "19.0.0",
    "react-aria-components": "^1.7.1",
    "react-dom": "19.0.0",
    "slugify": "1.6.6",
    "superjson": "2.2.2",
    "tailwind-merge": "^3.0.2",
    "tailwind-variants": "^1.0.0",
    "vinxi": "0.5.3",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "9.22.0",
    "@faker-js/faker": "9.6.0",
    "@ianvs/prettier-plugin-sort-imports": "4.4.1",
    "@types/node": "22.13.10",
    "@types/pg": "8.11.11",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@vitejs/plugin-react": "4.3.4",
    "drizzle-kit": "0.30.5",
    "eslint": "9.22.0",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "5.2.0",
    "prettier": "3.5.3",
    "tailwindcss": "^4.0.13",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-react-aria-components": "^2.0.0",
    "tsx": "4.19.3",
    "typescript": "5.8.2",
    "typescript-eslint": "8.26.1",
    "vite-tsconfig-paths": "5.1.4"
  }
}
