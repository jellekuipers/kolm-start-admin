{
  "name": "kolm-start-admin",
  "description": "A TanStack Start + better-auth admin starter, with Drizzle ORM, React Aria.",
  "author": "Jelle Kuipers <info@jellekuipers.com> (https://jellekuipers.com)",
  "version": "1.1.1",
  "type": "module",
  "keywords": [
    "better-auth",
    "biomejs",
    "drizzle-orm",
    "react-aria",
    "tanstack-start",
    "typescript"
  ],
  "scripts": {
    "build": "vite build && tsc --noEmit",
    "check": "npx @biomejs/biome check --write .",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:reset": "tsx src/db/reset.ts ",
    "db:seed:dev": "tsx app/db/seed.dev.ts ",
    "db:seed": "tsx src/db/seed.ts ",
    "db:studio": "drizzle-kit studio",
    "dev": "vite dev",
    "format": "npx @biomejs/biome format --write .",
    "lint": "npx @biomejs/biome lint --write .",
    "postinstall": "drizzle-kit push",
    "start": "vite start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@phosphor-icons/react": "^2.1.10",
    "@react-aria/optimize-locales-plugin": "^1.1.5",
    "@tanstack/match-sorter-utils": "^8.19.4",
    "@tanstack/react-form": "^1.23.4",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-router": "^1.132.7",
    "@tanstack/react-router-ssr-query": "^1.132.7",
    "@tanstack/react-start": "^1.132.13",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/router-devtools": "^1.132.11",
    "better-auth": "^1.3.18",
    "dotenv": "^17.2.2",
    "drizzle-orm": "^0.44.5",
    "drizzle-seed": "^0.3.1",
    "pg": "^8.16.3",
    "react": "^19.1.1",
    "react-aria": "^3.43.2",
    "react-aria-components": "^1.12.2",
    "react-dom": "^19.1.1",
    "slugify": "^1.6.6",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@faker-js/faker": "^10.0.0",
    "@tailwindcss/vite": "^4.1.13",
    "@types/node": "^24.5.2",
    "@types/pg": "^8.15.5",
    "@types/react": "^19.1.14",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "drizzle-kit": "^0.31.5",
    "tailwindcss": "^4.1.13",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-react-aria-components": "^2.0.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
