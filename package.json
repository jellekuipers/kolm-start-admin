    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@tanstack/react-router": "^1.143.3",
    "@tanstack/react-router-devtools": "^1.143.3",
    "@tanstack/react-router-ssr-query": "^1.143.3",
